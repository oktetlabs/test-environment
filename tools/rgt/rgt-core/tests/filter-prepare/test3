#!/bin/sh
# the next line restarts using tclsh \
exec tclsh "$0" "$@"

source "test3.tcl"
source "test_gen.tcl"

# The fileter should exclude everything, but TEST_ENTITY/TEST_USER
set test_cases [list \
    "A"   "B"         \
    "A"   ""          \
    ""    "B"         \
    ""    ""          \
    "ABA" "BBB"       \
    "TEST_ENTITY" "B" \
    "A" "TEST_USER"   \
]

foreach { entity user } $test_cases {
    check_rgt_entity_filter $entity $user "fail"
}

check_rgt_entity_filter "TEST_ENTITY" "TEST_USER" "pass"
