<html>
<head>
<title>Logs from @@%s:reporter@@</title>
<link rel='stylesheet' type='text/css' href='style.css'>

<script>
var hide = new Array();

function getAllLogs()
{
    if (document.getElementsByTagName)
    {
        return document.getElementsByTagName("div");
    }
    else if (document.getElementsById)
    {
        alert("Extend sources with getElementsById");
    }
    else
    {
        alert("Not supported");
    }
}

function open_filter_window()
{
    window.open("filter_conf_local.html",
                "parwin",
                "dependent,toolbar=no,menubar=no,status=no,width=400,height=300,screenX=300,screenY=100,scrollbars=no");
}

function activate_link(name, doc_name)
{
    parent.treeframe.activate_node(name);
    document.location.href=doc_name;
}

</script>

</head>

<body>

<table border='0' cellpadding='0' cellspacing='0' width='100%%'>
<tr class='tdheading'>
    <td width='100%%' class='heading' height='25'>
        &nbsp;&nbsp;&nbsp;Logs from @@%s:reporter@@
    </td>
</table>
<br>

<form name='form0'>
    <input type='button' value='Log Filter' 
           onClick='javascript:open_filter_window()'>
</form>
<br/>
