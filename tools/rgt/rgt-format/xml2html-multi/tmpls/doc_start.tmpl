<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Logs from @@%s:reporter@@</title>
<link rel='stylesheet' type='text/css' href='@@%s:shared_url@@style.css'>

<script src="node_@@%u:depth@@_@@%u:seq@@_log_names.js">
</script>

<script>

function getAllLogs()
{
    if (document.getElementsByTagName)
    {
        return document.getElementsByTagName("div");
    }
    else if (document.getElementsById)
    {
        alert("Extend sources with getElementsById");
    }
    else
    {
        alert("Not supported");
    }
}

function open_filter_window()
{
    window.top.filter_win = window.open(
        "@@%s:shared_url@@filter_conf.html",
        "parwin",
        "dependent,toolbar=no,menubar=no,status=no," +
        "width=400,height=300,top=0,left=1000,scrollbars=no");
}

function activate_link(name, doc_name, par_name)
{
    parent.treeframe.activate_node(name, par_name);
    document.location.href=doc_name;
}

function refresh_filter_window()
{
    if (typeof(window.top.filter_win) != "undefined")
    {
        // test to see if a window is still open
        if (!(window.top.filter_win.closed))
        {
            window.top.filter_win.location.reload();
        }
    }
}

</script>
<script src="http://ajax.googleapis.com/ajax/libs/dojo/1.4/dojo/dojo.xd.js" type="text/javascript"></script>
<script type="text/javascript">
function set_node(node, str)
{
  if(dojo.isString(node))
    node = dojo.byId(node);
  if(!node) return;
  node.style.display = str;
}
function toggle_node(node)
{
  if(dojo.isString(node))
    node = dojo.byId(node);
  if(!node) return;
  set_node(node, (node.style.display != 'none') ? 'none' : 'block');
}
function hide_node(node)
{
  set_node(node, 'none');
}
</script>

</head>

<body onLoad='javascript:refresh_filter_window()'>

<table border='0' cellpadding='0' cellspacing='0' width='100%%'>
<tr class='tdheading'>
    <td width='100%%' class='heading' height='25'>
        &nbsp;&nbsp;&nbsp;Logs from @@%s:reporter@@
    </td>
</table>
<br>

<form name='form0'>
    <input type='button' value='Log Filter' 
           onClick='javascript:open_filter_window()'>
</form>
<br/>
