#!/bin/sh
# the next line restarts using tclsh \
exec tclsh "$0" "$@"

source "test6.tcl"

set test_cases_pass [list \
    "Tester" "FlowNoRGT"  \
    "Tester" "Self"       \
    "A"  "Self"           \
    ""   "Self"           \
    "ABS"    "Self"       \
    "Logger"    "Self"    \
]

set test_cases_fail [list \
    "A"   "B"         \
    "A"   ""          \
    ""    "B"         \
    ""    ""          \
    "ABA" "BBB"       \
    "Tester" "ABS"    \
    "Tester" ""       \
]

set any_log_level 1
source "test_gen.tcl"
