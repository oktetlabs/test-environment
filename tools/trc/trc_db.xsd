    <element name="test" type="Test"/>

    <complexType name="Test">
        <sequence>
            <element name="objective" type="string"/>
            <element name="arg" minOccurs="0" maxOccurs="unbounded">
                <simpleContent>
                    <extension base="string">
                        <attribute name="name" type="token"/>
                    </extension>
                </simpleContent>
            </element>
            <element name="notes" type="string"/>
            <element name="test" type="Test" minOccurs="0"
                                             maxOccurs="unbounded"/>
        </sequence>
        <attribute name="name" type="token" use="required"/>
        <attribute name="package" type="token" default="false"/>
        <attribute name="result" use="required">
            <simpleType>
                <restriction base="token">
                    <enumeration value="pass"/>
                    <enumeration value="fail"/>
                </restriction>
            </simpleType>
        </attribute>
    </complexType>

</schema> 
