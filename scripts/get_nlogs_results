#!/usr/bin/perl

use strict;
use warnings;
use IPC::Open2;
use File::Basename;

my $sh_path;
my $sh_ld_library_path;

my $pid = open2(*RDR, undef, dirname(__FILE__)."/../guess_to_perl.sh");
while (<RDR>)
{
    eval($_);
}

$ENV{"PATH"} = $sh_path;
$ENV{"LD_LIBRARY_PATH"} = $sh_ld_library_path;

exec("te_get_nlogs_results", @ARGV);
