<?xml version="1.0"?>
<package version="1.0">
    <description>General ACSE test</description>
    <author mailto="Konstantin.Abramenko@oktetlabs.ru"/>
    <session track_conf="silent">
        <var name="ta_acse" handdown="true">
            <value>Agt_A</value>
        </var>

        <prologue>
            <script name="prologue"/>
            <arg name="ta_acse" ref="ta_acse"/>
        </prologue>

        <epilogue>
            <script name="epilogue"/>
            <arg name="ta_acse" ref="ta_acse"/>
        </epilogue>

        <run>
            <script name="acse"/>
            <arg name="ta_acse" ref="ta_acse"/>
        </run> 

        <run>
            <script name="download"/>
            <arg name="ta_acse" ref="ta_acse"/>
        </run> 

    </session>
</package>

