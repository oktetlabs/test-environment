<?xml version="1.0"?>
<package version="1.0">
    <description>General ACSE test</description>
    <author mailto="Konstantin.Abramenko@oktetlabs.ru"/>
    <session track_conf="silent">
        <arg name="ta_acse" handdown="true">
            <value>Agt_A</value>
        </arg>

        <prologue>
            <script name="prologue"/>
            <arg name="ta_acse"/>
        </prologue>

        <epilogue>
            <script name="epilogue"/>
            <arg name="ta_acse"/>
        </epilogue>

        <run>
            <script name="acse"/>
            <arg name="ta_acse"/>
        </run>

        <run>
            <script name="download"/>
            <arg name="ta_acse"/>
            <arg name="file_type" list="">
                <value>3 Vendor Configuration File</value>
                <value>5 Some Stuff</value>
                <value>1 Firmware Upgrade Image</value>
            </arg>
            <arg name="url" list="">
                <value>http://10.20.1.1:8080/file.cfg</value>
                <value>http://10.20.1.1:8080/stuff.txt</value>
                <value>http://10.20.1.1:8080/firmware_std.bin</value>
            </arg>
        </run>

        <run>
            <script name="get_param"/>
            <arg name="ta_acse"/>
        </run>

        <run>
            <script name="set_param"/>
            <arg name="ta_acse"/>
        </run>

        <run>
            <script name="change_wan"/>
            <arg name="ta_acse"/>
        </run>

        <run>
            <script name="hold_req"/>
            <arg name="ta_acse"/>
        </run>

        <run>
            <script name="add_object"/>
            <arg name="ta_acse"/>
        </run>


    </session>
</package>

