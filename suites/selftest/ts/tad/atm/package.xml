<package version="1.0">

    <description>CSAP ATM layer tests</description>

    <author mailto="Andrew.Rybchenko@oktetlabs.ru"/>

    <req id="TAD_ATM" sticky="true"/>

    <session>
        <arg name="env">
            <value>{'iut_host'{{'pco_iut':IUT},addr:'iut_addr':inet:unicast},{{'pco_tst':tester},addr:'tst_addr':inet:unicast}}</value>
        </arg>

        <run>
            <script name="simple_send"/>
            <arg name="gfc">
                <value>0</value>
                <value>1</value>
                <value>8</value>
            </arg>
            <arg name="vpi">
                <value>200</value>
            </arg>
            <arg name="vci">
                <value>1000</value>
            </arg>
            <arg name="congestion" type="boolean"/>
            <arg name="clp" type="boolean"/>
        </run>

        <run>
            <script name="simple_recv"/>
            <arg name="type">
                <value>uni</value>
<!--
                <value reqs="TIME_CONSUMING">nni</value>
-->
            </arg>
            <arg name="csap_vpi">
                <value>unspec</value>
<!--
                <value reqs="TIME_CONSUMING">match</value>
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="csap_vci">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
-->
                <value>match</value>
<!--
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="csap_congestion">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
                <value reqs="TIME_CONSUMING">match</value>
-->
                <value>nomatch</value>
            </arg>
            <arg name="csap_clp">
                <value>unspec</value>
<!--
                <value reqs="TIME_CONSUMING">match</value>
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="ptrn_gfc">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
-->
                <value>match</value>
<!--
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="ptrn_vpi">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
                <value reqs="TIME_CONSUMING">match</value>
-->
                <value>nomatch</value>
            </arg>
            <arg name="ptrn_vci">
                <value>unspec</value>
<!--
                <value reqs="TIME_CONSUMING">match</value>
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="ptrn_payload_type">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
-->
                <value>match</value>
<!--
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="ptrn_congestion">
<!--
                <value reqs="TIME_CONSUMING">unspec</value>
                <value reqs="TIME_CONSUMING">match</value>
-->
                <value>nomatch</value>
            </arg>
            <arg name="ptrn_clp">
                <value>unspec</value>
<!--
                <value reqs="TIME_CONSUMING">match</value>
                <value reqs="TIME_CONSUMING">nomatch</value>
-->
            </arg>
            <arg name="gfc">
                <value>0</value>
                <value>1</value>
                <value>8</value>
            </arg>
            <arg name="vpi">
                <value>129</value>
            </arg>
            <arg name="vci">
                <value>32769</value>
            </arg>
            <arg name="payload_type">
                <value>0</value>
                <value>1</value>
                <value>7</value>
            </arg>
            <arg name="congestion" type="boolean">
                <value>FALSE</value>
                <value>TRUE</value>
            </arg>
            <arg name="clp" type="boolean">
                <value>FALSE</value>
                <value>TRUE</value>
            </arg>
        </run>

        <run>
            <script name="aal5_send">
                <req id="ATM_AAL5"/>
            </script>
            <arg name="gfc">
                <value>8</value>
            </arg>
            <arg name="vpi">
                <value>1</value>
            </arg>
            <arg name="vci">
                <value>1</value>
            </arg>
            <arg name="congestion" type="boolean">
                <value>FALSE</value>
            </arg>
            <arg name="clp" type="boolean">
                <value>TRUE</value>
            </arg>
            <arg name="cpcs_uu">
                <value>127</value>
            </arg>
            <arg name="cpi">
                <value>192</value>
            </arg>
        </run>

   </session>
</package>
