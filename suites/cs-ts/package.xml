<?xml version="1.0"?>
<package version="1.0">

  <description>Tests on Configurator functionality.</description>

  <author mailto="Konstantin.Abramenko@oktetlabs.ru"/>

  <session track_conf="silent">

    <run>
      <script name="vlans">
        <req id="CS_VLAN"/>
      </script>
      <arg name="env">
        <value>{'host_csap'{if:'csap_if'}}</value>
      </arg>  
    </run>
    
    <run>
      <script name="phy" track_conf="nohistory">
      </script>
      <arg name="ta">
        <value>Agt_B</value>
      </arg>
      <arg name="iface_name">
        <value>eth1</value>
      </arg>
      <arg name="second_ta">
        <value>Agt_A</value>
      </arg>
      <arg name="second_iface">
        <value>eth3</value>
      </arg>
      <arg name="speed">
        <value>10</value>
      </arg>
      <arg name="duplex">
        <value>0</value>
      </arg>
      <arg name="speed_adver">
        <value>100</value>
      </arg>
      <arg name="duplex_adver">
        <value>1</value>
      </arg>
    </run>

  </session>
</package>
