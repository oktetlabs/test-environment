<?xml version="1.0"?>
<package version="1.0">
    
    <description>Tests on TAD SNMP functionality.</description>

    <author mailto="Konstantin.Abramenko@oktetlabs.ru"/>

    <session random="true">
        <var name="ta" handdown="true">
            <value>Agt_A</value>
        </var>
        <var name="snmp_agt" handdown="true">
            <value>192.168.37.14</value>
        </var>
        <var name="snmp_version" handdown="true">
            <value>2</value>
        </var>

        <run>
            <script name="snmp_gets">
                <req id="SNMP"/>
            </script>
            <arg name="ta"/>
            <arg name="snmp_agt"/>
            <arg name="snmp_version"/>
            <arg name="mib_name">
                <value>SNMPv2-MIB.txt</value>
            </arg>
            <arg name="mib_object">
                <value>sysDescr</value>
            </arg>
        </run>

        <run>
            <script name="snmp_get_if_table">
                <req id="SNMP"/>
            </script>
            <arg name="ta"/>
            <arg name="snmp_agt"/>
            <arg name="snmp_version"/>
            <arg name="mib_name">
                <value>SNMPv2-MIB.txt</value>
            </arg>
        </run>
    </session>

</package>
