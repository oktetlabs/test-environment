<?xml version="1.0"?>
<package version="1.0">
    
    <description>CLI CSAP tests</description>
    
    <author mailto="Konstantin.Abramenko@oktetlabs.ru"/>
    <req id="TAD_ETH" sticky="true"/>

    <session>
        <run>
            <script name="caught_any">
                <req id="TAD_ETH"/> 
            </script>
            <arg name="num_pkts">
                <value>2</value>
                <value>5</value>
            </arg>
            <arg name="timeout"> 
                <value>200</value><!-- in milliseconds -->
            </arg>
            <arg name="pass_results">
                <value>FALSE</value>
                <value>TRUE</value>
            </arg>
            <arg name="dump_packets">
                <value>FALSE</value>
                <value>TRUE</value>
            </arg>
            <arg name="blocked_mode">
                <value>FALSE</value>
                <value>TRUE</value>
            </arg>
        </run>
        <run>
            <script name="simple_recv_send">
                <req id="TAD_ETH"/> 
            </script>
        </run>
        <run>
            <script name="vlan_recv_send">
                <req id="TAD_ETH"/> 
            </script>
        </run>
   </session>
</package>
