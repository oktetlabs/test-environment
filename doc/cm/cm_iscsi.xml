    <!-- iSCSI parameters. See RFC3720 for more information.
         -->
    
    <!-- iSCSI Initiator configuring
         Name: iSCSI Target name
         Value: up 5 - up the connection with CID 5
                down 5 - down the connection with CID 5
         -->
    <object oid="/agent/iscsi" access="read_create" type="string"/>

    <!-- iSCSI Initiator MaxConnections parameter
         Name: empty
         Value: Maximum connections between the Initiator and the Target in
         one session (1..65535). (Default : 1)
         -->
    <object oid="/agent/iscsi/max_connections" access="read_write"
        type="integet"/>
    
    <!-- iSCSI Initiator InitialR2T parameter (per target)
         Name: empty
         Value: <boolean-value> (Default : Yes)
         -->
    <object oid="/agent/iscsi/initial_r2t" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator HeaderDigest parameter (per target)
         Name: empty
         Value: <list-of-values> (Default : None)
         -->
    <object oid="/agent/iscsi/header_digest" access="read_write"
        type="string"/>

    <!-- iSCSI Initiator DataDigest parameter (per target)
         Name: empty
         Value: <list-of-values> (Default : None)
         -->
    <object oid="/agent/iscsi/data_digest" access="read_write"
        type="string"/>
    
    <!-- iSCSI Initiator ImmediateData parameter (per target)
         Name: empty
         Value: <boolean-value> (Default : Yes)
         -->
    <object oid="/agent/iscsi/immediate_data" access="read_write"
        type="string"/>
    
    <!-- iSCSI Initiator MaxRecvDataSegmentLength parameter (per target)
         Name: empty
         Value: <numerical-value-512-to-(2**24-1)> (Default : 8192)
         -->
    <object oid="/agent/iscsi/max_recv_data_segment_length" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator FirstBurstLength parameter (per target)
         Name: empty
         Value: <numerical-value-512-to-(2**24-1)> (Default : 64K)
         -->
    <object oid="/agent/iscsi/first_burst_length" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator MaxBurstLength parameter (per target)
         Name: empty
         Value: <numerical-value-512-to-(2**24-1)> (Default : 256K)
         -->
    <object oid="/agent/iscsi/max_burst_length" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator DefaultTime2Wait parameter (per target)
         Name: empty
         Value: <numerical-value-0-to-3600> (Default : 2)
         -->
    <object oid="/agent/iscsi/default_time2wait" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator DefaultTime2Retain parameter (per target)
         Name: empty
         Value: <numerical-value-0-to-3600> (Default : 20)
         -->
    <object oid="/agent/iscsi/default_time2retain" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator MaxOutstandingR2T parameter (per target)
         Name: empty
         Value: <numerical-value-from-1-to-65535> (Default : 1)
         -->
    <object oid="/agent/iscsi/max_outstanding_r2t" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator DataPDUInOrder parameter (per target)
         Name: empty
         Value: <boolean-value> (Default : Yes)
         -->
    <object oid="/agent/iscsi/data_pdu_in_order" access="read_write"
        type="string"/>
    
    <!-- iSCSI Initiator DataSequenceInOrder parameter (per target)
         Name: empty
         Value: <boolean-value> (Default : Yes)
         -->
    <object oid="/agent/iscsi/data_sequence_in_order" access="read_write"
        type="string"/>
    
    <!-- iSCSI Initiator Error Recovery Level parameter (per target)
         Name: empty
         Value: <numerical-value-0-to-2> (Default : 0)
         -->
    <object oid="/agent/iscsi/error_recovery_level" access="read_write"
        type="integer"/>
    
    <!-- iSCSI Initiator Session type parameter (per target)
         Name: empty
         Value: Normal or Discovery (Default : Normal)
         -->
    <object oid="/agent/iscsi/session_type" access="read_write"
        type="string"/>
    
    <!-- iSCSI Initiator Authentication method (per target)
         Name: empty
         Value: CHAP or None or "CHAP,None"
         -->
    <object oid="/agent/iscsi/auth_method" access="read_write"
        type="string"/>

    <!-- iSCSI Initiator target name (is sent in the Login Request PDU)
         Name: empty
         Value: iqn.2004-01.com:0 - default (iSCSI-name-value)
         -->
    <object oid="/agent/iscsi/target_name" access="read_write"
        type="string"/>

    <!-- iSCSI Initiator name which is sent in the Login Request PDU
         Name: empty
         Value: iSCSI-name-value
         -->
    <object oid="/agent/iscsi/initiator_name" access="read_write"
        type="string"/>

    <!-- iSCSI Initiator Alias
         Name: empty
         Value: iSCSI-local-name-value
         -->
    <object oid="/agent/iscsi/initiator_alias" access="read_write"
        type="string"/>

    <!-- iSCSI Target address 
         Name: empty
         Value: ip address of the target to connect to
         -->
    <object oid="/agent/iscsi/target_address" access="read_write"
        type="address"/>

    <!-- iSCSI Target port
         Name: empty
         Value: port of the target to connect to (Default : 3260)
         -->
    <object oid="/agent/iscsi/target_port" access="read_write"
        type="integer"/>

    <!-- iSCSI Target 
         Name: empty
         Value: none 
         -->
    <object oid="/agent/iscsi_target" access="read_only"
        type="none"/> 

    <!-- iSCSI Target CHAP stuff 
         Name: empty
         Value: 0 (disable)
                1 (enable)
         -->
    <object oid="/agent/iscsi_target/chap" access="read_write"
        type="integer"/>

    <!-- iSCSI target local secret
         Name: none
         Value: string 
         -->    
    <object oid="/agent/iscsi_target/chap/lx" access="read_write"
        type="string"/>

    <!-- iSCSI Target local name
         Name: none
         Value: string
         -->    
    <object oid="/agent/iscsi_target/chap/ln" access="read_write"
        type="string"/>

    <!-- iSCSI Target length of bytes in the CHAP challenge
         Name: none
         Value: <numerical-value-256-to-1024> (Default: none)
         -->
    <object oid="/agent/iscsi_target/chap/cl" access="read_write"
         type="integer"/>

    <!-- iSCSI Target format encoding
         Name: none
         Value: 0 (hex encoding - default)
                1 (64-base encoding)
         -->
    <object oid="/agent/iscsi_target/chap/b" access="read_write"
         type="integer"/>     

    <!-- iSCSI Target authentication forced
         Name: none
         Value: 0 (disabled)
                1 (enabled)
         -->     
    <object oid="/agent/iscsi_target/chap/t" access="read_write"
         type="integer"/>
    
    <!-- iSCSI Target peer secret
         Name: none
         Value: string
         -->
    <object oid="/agent/iscsi_target/chap/t/px" access="read_write"
         type="string"/>

    <!-- iSCSI Target peer name
         Name: none
         Value: string
         -->
    <object oid="/agent/iscsi_target/chap/t/pn" access="read_write"
         type="string"/>     
         
</backup>
