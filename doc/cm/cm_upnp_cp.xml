<?xml version="1.0"?>
<backup>

    <!--
        This configuration model is intended for describing of configuration
        related to UPnP Control Points on Agents
    -->

    <!--
        Copyright (C) 2003-2018 OKTET Labs.

        Author: Ivan Melnikov E-mail: Ivan.Melnikov@oktetlabs.ru
    -->

    <!-- Objects registered by Configurator itself -->

    <!-- Root object for the UPnP Control Point process
         Name: empty -->
    <object oid="/agent/upnp_cp" access="read_only" type="string"/>

    <!-- Enable the UPnP Control Point
         Name:  empty
         Value: 0 (disabled) or 1 (enabled) -->
    <object oid="/agent/upnp_cp/enable" access="read_write" type="integer"/>

    <!-- Search Target for UPnP devices and/or services
         Name:  empty
         Value: a search target string; for example, "ssdp:all" -->
    <object oid="/agent/upnp_cp/target" access="read_write" type="string"/>

    <!-- Network interface which be used by UPnP Control Point to interact
         with UPnP devices
         Name:  empty
         Value: a network interface -->
    <object oid="/agent/upnp_cp/iface" access="read_write" type="string"/>

</backup>
