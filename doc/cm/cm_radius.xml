<?xml version="1.0" ?>
<backup>
    <!-- RADIUS server enabling/disabling
    1 - enable
    0 - disable
    -->
    <object oid="/agent/radiusserver" access="read_write" type="integer" />
    
    <object oid="/agent/radiusserver/auth_port" access="read_write" type="integer"/>
    <object oid="/agent/radiusserver/acct_port" access="read_write" type="integer"/>
    <object oid="/agent/radiusserver/net_addr" access="read_write"  type="address"/>
    
    <object oid="/agent/radiusserver/client" access="read_create" type="none"/>
    <object oid="/agent/radiusserver/client/secret" access="read_write" 
        type="string"/>
    
    <!--
    Instance name is "User name" value,
    Instance value - 0 - Reject user, 1 - Accept user
    -->
    <object oid="/agent/radiusserver/user" access="read_create" type="none"/>
    
    <!--
    Attribute checks for a given user
    Instance name - empty,
    Instance value - a list of the form:
                     AttributeName=Value[,AttributeName=Value]
    -->
    <object oid="/agent/radiusserver/user/check" access="read_write" type="string"/>
    
    <!--
    Instance name - empty
    Instance value - same as the above
    Attributes to set for Access-Challenge and Access-Accept resp.
    Each check element is mapped onto "Attribute = Value" directive in
    configuration file.
     -->
    <object oid="/agent/radiusserver/user/challenge-attrs" access="read_write"  type="string"/>
    <object oid="/agent/radiusserver/user/accept-attrs" access="read_write"  type="string"/>

</backup>
