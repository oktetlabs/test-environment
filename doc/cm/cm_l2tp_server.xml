    <!-- Root object for the L2TP server
         Name: empty -->
    <object oid="/agent/l2tp_server/" 
        access="read_write" type="integer"/>
    <!-- ppp interface which starts when connection
         between server and client is established
         Name: empty -->
    <object oid="/agent/l2tp_server/pppX" 
        access="read_create" type="integer"/>
    <!-- L2TP server local address start
         Name: empty -->
    <object oid="/agent/l2tp_server/pppX/l2tp_server_addr" 
        access="read_write" type="integer"/>
    <!-- L2TP client address
         Name: empty -->
    <object oid="/agent/l2tp_server/pppX/l2tp_client_addr" 
        access="read_write" type="integer"/>
</backup>
