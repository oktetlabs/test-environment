    <!-- Root object of wpa_supplicant configuration for the interface.
         Name: empty -->
    <object oid="/agent/interface/supplicant" access="read_write" type="integer"/>
    <!-- EAP identity: Double-quoted ASCII
         Name: empty -->
    <object oid="/agent/interface/supplicant/identity" access="read_write" type="string"/>
    <!-- EAP method: "eap-md5", "eap-tls" etc.
         Name: empty -->
    <object oid="/agent/interface/supplicant/cur_method" access="read_write" type="string"/>
    <!-- Authentication protocol: "WPA", "WPA2" or "RSN"
         Name: empty -->
    <object oid="/agent/interface/supplicant/proto" access="read_write" type="string"/>
    <!-- Key management: "WPA-PSK", "WPA-EAP" or "NONE"
         Name: empty -->
    <object oid="/agent/interface/supplicant/key_mgmt" access="read_write" type="string"/>
    <!-- WEP key0: Double-quoted string of hex digits
         Name: empty -->
    <object oid="/agent/interface/supplicant/wep_key0" access="read_write" type="string"/>
    <!-- WEP key1: Double-quoted string of hex digits
         Name: empty -->
    <object oid="/agent/interface/supplicant/wep_key1" access="read_write" type="string"/>
    <!-- WEP key2: Double-quoted string of hex digits
         Name: empty -->
    <object oid="/agent/interface/supplicant/wep_key2" access="read_write" type="string"/>
    <!-- WEP key3: Double-quoted string of hex digits
         Name: empty -->
    <object oid="/agent/interface/supplicant/wep_key3" access="read_write" type="string"/>
    <!-- Default WEP key index 0..3, double-quoted digit symbol
         Name: empty -->
    <object oid="/agent/interface/supplicant/wep_tx_keyidx" access="read_write" type="string"/>
    <!-- Authentication algorithm "OPEN" or "SHARED" or "LEAP"
         Name: empty -->
    <object oid="/agent/interface/supplicant/auth_alg" access="read_write" type="string"/>
    <!-- RSN group cipher: "TKIP", "CCMP" or "TKIP CCMP"
         Name: empty -->
    <object oid="/agent/interface/supplicant/group" access="read_write" type="string"/>
    <!-- RSN pairwise cipher: "TKIP", "CCMP" or "TKIP CCMP"
         Name: empty -->
    <object oid="/agent/interface/supplicant/pairwise" access="read_write" type="string"/>
    <!-- RSN personal security key, double quoted ASCII 8..63 symbols
         Name: empty -->
    <object oid="/agent/interface/supplicant/psk" access="read_write" type="string"/>
    <!-- Node with EAP-TLS settings
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-tls" access="read_write" type="none"/>
    <!-- EAP-TLS path to user certificate file, double quoted filename
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-tls/cert" access="read_write" type="string"/>
    <!-- EAP-TLS path to user private key file, double quoted filename
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-tls/key" access="read_write" type="string"/>
    <!-- EAP-TLS password for user private key, double-quoted ASCII
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-tls/key_passwd" access="read_write" type="string"/>
    <!-- EAP-TLS path to root certificate file, double quoted filename
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-tls/root_cert" access="read_write" type="string"/>
    <!-- Node with EAP-MD5 settings
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-md5" access="read_write" type="none"/>
    <!-- EAP-MD5 username, double-quoted ASCII
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-md5/username" access="read_write" type="string"/>
    <!-- EAP-MD5 password, double-quoted ASCII
         Name: empty -->
    <object oid="/agent/interface/supplicant/eap-md5/passwd" access="read_write" type="string"/>
    <!-- String with additional startup options for
         wpa_supplicant, double-quoted ASCII
         Name: empty -->
    <object oid="/agent/interface/supplicant/optstr" access="read_write" type="string"/>
</backup>
