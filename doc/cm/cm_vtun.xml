    <!-- ************** Network configuration ************** -->

    <!-- Vtun session
         Name: "AgtX-AgtY-N"-like -->
    <object oid="/agent/vtund" access="read_create" type="none"/>
    
    <!-- Vtund listening port
         Name: empty -->
    <object oid="/agent/vtund/port" access="read_create" type="integer"/>

    <!-- General timeout
         Name: empty -->
    <object oid="/agent/vtund/timeout" access="read_create" type="integer"/>

    <!-- Vtund Persist mode (try to recoonnect after connection termination
         Name: empty -->
    <object oid="/agent/vtund/persist" access="read_create" type="boolean"/>

    <!-- Vtund syslog specification
         Name: empty -->
    <object oid="/agent/vtund/syslog" access="read_create" type="string"/>

    <!-- Path to ppp program
         Name: empty -->
    <object oid="/agent/vtund/ppp" access="read_create" type="string"/>

    <!-- Path to ifconfig program
         Name: empty -->
    <object oid="/agent/vtund/ifconfig" access="read_create" type="string"/>

    <!-- Path to route program
         Name: empty -->
    <object oid="/agent/vtund/route" access="read_create" type="string"/>

    <!-- Path to ip program
         Name: empty -->
    <object oid="/agent/vtund/ip" access="read_create" type="string"/>

    <!-- Path to firewall program
         Name: empty -->
    <object oid="/agent/vtund/firewall" access="read_create" type="string"/>

    <!-- Password for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/type" access="read_create" type="integer"/>

    <!-- Type of Vtund connection
         Name: tun, ether, tty or pipe -->
    <object oid="/agent/vtund/type" access="read_create" type="integer"/>

    <!-- Vtund device name
         Name: "tun1"-like -->
    <object oid="/agent/vtund/device" access="read_create" type="none"/>

    <!-- Protocol used for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/proto" access="read_create" type="none"/>

    <!-- Compression method to use for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/compress" access="read_create" type="none"/>

    <!-- Encryption method to use for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/encrypt" access="read_create" type="none"/>

    <!-- Enable/disable connection keep-alive for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/keepalive" access="read_create" type="none"/>

    <!-- Enable/disable connection statistics for Vtund connection
         Name: empty -->
    <object oid="/agent/vtund/stats" access="read_create" type="none"/>

    <!-- Vtund connection speed
         Name: <In/Out> or <In>:<Out> -->
    <object oid="/agent/vtund/speed" access="read_create" type="none"/>

    <!-- Source network interface name for Vtund connection
         Name: "eth0"-like -->
    <object oid="/agent/vtund/iface" access="read_create" type="none"/>

    <!-- Wheter to allow multiple connections
         Name: empty -->
    <object oid="/agent/vtund/multi" access="read_create" type="none"/>

</backup>
